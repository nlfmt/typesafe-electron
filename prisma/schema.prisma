generator client {
  provider = "prisma-client-js"
  output   = "../build/resources/app/node_modules/@prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./../resources/db.sqlite"
}

// define your models here
model Example {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}
